    <style>
        /* Global styles */
        body {
            font-family: 'Inter', sans-serif;
        }

        /* Navbar styles */
        .navbar {
            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
        }

        .navbar-brand {
            font-size: 1.5rem;
            font-weight: bold;
        }

        .navbar-nav .nav-link {
            font-size: 1.1rem;
            margin-right: 1rem;
        }

        .navbar-toggler {
            border: none;
        }

        .navbar-toggler-icon {
            color: #007bff;
            font-size: 1.2rem;
        }

        /* Hero section styles */
        .hero {
            background-image: url('https://images.unsplash.com/photo-1600585154340-be6161a56a0c');
            background-size: cover;
            background-position: center;
            height: 100vh;
            color: white;
            text-align: center;
            display: flex;
            justify-content: center;
            align-items: center;
            flex-direction: column;
        }

        .hero h1 {
            font-size: 3rem;
            font-weight: bold;
        }

        .hero p {
            font-size: 1.2rem;
            margin-bottom: 2rem;
        }

        .btn-primary {
            background-color: #007bff;
            border: none;
            padding: 0.75rem 1.5rem;
            font-size: 1.1rem;
            border-radius: 5px;
        }

        .btn-primary:hover {
            background-color: #0056b3;
        }

        /* About section styles */
        .about {
            padding: 4rem 0;
        }

        .about h2 {
            font-size: 2.5rem;
            margin-bottom: 2rem;
        }

        .about p {
            font-size: 1.2rem;
        }

        /* Services section styles */
        .services {
            padding: 4rem 0;
            background-color: #f8f9fa;
        }

        .services h2 {
            font-size: 2.5rem;
            margin-bottom: 2rem;
        }

        .card {
            border: none;
            box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
            transition: transform 0.3s ease, box-shadow 0.3s ease;
        }

        .card:hover {
            transform: translateY(-10px);
            box-shadow: 0 8px 16px rgba(0, 0, 0, 0.2);
        }

        .card-img-top {
            height: 200px;
            object-fit: cover;
        }

        .card-title {
            font-size: 1.3rem;
            font-weight: bold;
        }

        .card-text {
            font-size: 1rem;
        }

        /* Carousel item animation */
        .carousel-item {
            transition: transform 0.5s ease;
        }

        /* Footer styles */
        footer {
            padding: 2rem 0;
            background-color: #1c2331;
            color: white;
        }

        footer a {
            color: #7c4dff;
        }

        footer a:hover {
            color: #1c2331;
            text-decoration: none;
        }

        footer hr {
            border-top: 1px solid #7c4dff;
            width: 60px;
            margin: 0 auto;
            margin-bottom: 1rem;
        }
    </style>
</head>
